[MULTICRAB]
cfg=../crab.cfg

[COMMON]
CRAB.scheduler=condor
CMSSW.lumis_per_job=500

##########################
##Run2011A May 10 ReReco##
##########################

[DiLeptonJet-V360B-DoubleElectron-Run2011A-May10ReReco-v1]
